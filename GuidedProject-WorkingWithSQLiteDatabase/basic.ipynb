   "source": [
    "# Hozzon létre egy kapcsolatpéldányt - Create a connection instance\n",
    "myConnection = sqlite3.connect('factbook.db')\n",
    "\n",
    "# kurzorobjektum létrehozása - create a Cursor object\n",
    "myCursor = myConnection.cursor()\n",
    "\n",
    "# Tárolja a lekérdezést stringként - Store the query as string\n",
    "myQuery = 'SELECT name, population, area FROM facts ORDER BY population DESC LIMIT 10;'\n",
    "\n",
    "# Lekérdezés az adatkészletből - Query the dataset\n",
    "myCursor.execute(myQuery)\n",
    "\n",
    "# Az eredmények letöltése - Fetch the results \n",
    "Large_countries = myCursor.execute(myQuery).fetchall()\n",
    "print(Large_countries)\n",
    "\n",
    "# A kapcsolat lezárása - Close the connection\n",
    "myConnection.close()"
   ]
